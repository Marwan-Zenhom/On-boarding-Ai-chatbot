{
    "name": "novatech-ai-onboarding",
    "version": "1.0.0",
    "description": "NovaTech AI Onboarding Assistant - Enterprise Agentic Chatbot",
    "private": true,
    "scripts": {
        "setup": "npm install && npm run setup:backend && npm run setup:frontend",
        "setup:backend": "cd backend && npm install",
        "setup:frontend": "cd frontend && npm install",
        "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
        "dev:backend": "cd backend && npm run dev",
        "dev:frontend": "cd frontend && npm start",
        "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
        "start:backend": "cd backend && npm start",
        "start:frontend": "cd frontend && npm run build && serve -s frontend/build",
        "load-kb": "cd backend && npm run load-kb"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    },
    "author": "Marwan Zenhom",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/On-boarding-Ai-chatbot.git"
    },
    "keywords": [
        "ai",
        "chatbot",
        "onboarding",
        "gemini",
        "agentic",
        "rag",
        "supabase",
        "react"
    ]
}